.list-item.container-fluid{ id: "treino-#{treino.nome_treino}" }
  %tr
    %td.treino-cpf-col
      =link_to treino.cpf, aluno_path(treino.aluno.id)
    %td.treino-nome-aluno-col
      =link_to treino.aluno.nome_aluno, aluno_path(treino.aluno.id)
    %td.treino-nome-col
      =treino.nome_treino
    %td.treino-inicio-col
      =treino.data_inicio
    %td.treino-fim-col
      =treino.data_fim